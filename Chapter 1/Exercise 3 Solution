#include <iostream> // Allows Input/Output
using namespace std; //Declares the use of the standard mainspace

int main() {
    /*int*/ double numberOne = 50; //Assigns the variable numberOne with the double 50
    /*int*/ double numberTwo = 7; //Assigns the variable numberTwo with the double 7
    cout << numberOne/numberTwo << endl; //Divides the contents of variable numberOne by variable numberTwo, and outputs the result to the console

  //Error with the divison was the variables were declared as integers, not doubles. The original variable could not handle the decimal places of the result of the output.

    return 0; //Termminates the program
}
